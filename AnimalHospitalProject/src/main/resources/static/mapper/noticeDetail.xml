                        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.human.anihospital.mapper.NoticeDetailMapper">
	<update id="deleteNotice" parameterType="int">
		 <![CDATA[
		    delete from notice
			WHERE
				seq_notice = #{seq_notice}
		]]>
	</update>
</mapper>               